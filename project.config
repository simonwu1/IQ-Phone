[project]
	description = for training
[access]
	inheritFrom = All-Projects
[access "refs/*"]
	owner = group user/simonwu1
[access "refs/for/refs/*"]
	read = group user/simonwu1
	push = group user/simonwu1
	label-Code-Review = -2..+2 group user/simonwu1
	label-Verified = -1..+1 group user/simonwu1
	rebase = group user/simonwu1
	removeReviewer = group user/simonwu1
	submit = group user/simonwu1
[access "refs/heads/*"]
	read = group user/simonwu1
	create = group user/simonwu1
	push = +force group user/simonwu1
	pushMerge = group user/simonwu1
[access "refs/tags/*"]
	push = +force group user/simonwu1
	pushTag = group user/simonwu1
	pushSignedTag = group user/simonwu1
